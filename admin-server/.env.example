# Copy this file to admin-server/.env and fill values. Do NOT commit .env.

# Azure OAuth
AZURE_CLIENT_ID=
AZURE_CLIENT_SECRET=
AZURE_TENANT_ID=
AZURE_REDIRECT_URI=

# Database (local sqlite)
# DATABASE_URL="file:./dev.db"

# OR Supabase / Postgres (example)
# Replace <username>, <password>, <host>, <port>, <database> with values from
# Supabase → Settings → Database → Connection string
# Make sure to include sslmode=require and schema if needed.
DATABASE_URL="postgresql://<username>:<password>@<host>:<port>/<database>?schema=public&sslmode=require"

# Example (do NOT paste your real secret here):
# DATABASE_URL="postgresql://postgres:abc123@db.abcdef.supabase.co:5432/postgres?schema=public&sslmode=require"

PORT=3000
NODE_ENV=development

# Admin
ADMIN_EMAIL=

# Stripe
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# Supabase
SUPABASE_URL="https://<your-project>.supabase.co"
# Public (publishable / anon) key used by client-side code
SUPABASE_ANON_KEY="sb_publishable_XXXXXXXXXXXXXXXXXXXXXXXXXXXX"
# Service role key for server-side (keep secret)
SUPABASE_SERVICE_ROLE_KEY="<SERVICE_ROLE_KEY>"

# Dev auth (disable in prod)
ENABLE_DEV_LOGIN=true
DEV_ADMIN_PASSWORD=
# DEV_ADMIN_EMAIL=

# Analytics
GTM_CONTAINER_ID=
META_PIXEL_ID=
META_ACCESS_TOKEN=
TIKTOK_PIXEL_ID=
TIKTOK_ACCESS_TOKEN=